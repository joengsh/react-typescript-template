    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "tw-elements": "^1.0.0-alpha13",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "start": "webpack serve --port 3000 --open --config webpack.dev.js",
    "build": "npm run build:clean && tsc && tsc -m es2020 --outDir lib-esm && webpack --config webpack.lib.js",
    "build:clean": "shx rm -rf _bundles lib lib-esm",
    "report": "WEBPACK_REPORT=true webpack --profile --config webpack.lib.js --json > stats.json; webpack-bundle-analyzer ./stats.json",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "jest": "jest",
    "jest:changed": "jest --onlyChanged",
    "jest:staged": "jest --env=jsdom --findRelatedTests --passWithNoTests",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "jest:coverage:watch": "jest --coverage --watch",
    "cy:open": "cypress open",
    "cy:run-unit": "cypress run --component --browser chrome",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "npm run jest && npm run cy:run-unit",
    "coverage": "shx rm -rf coverage && npm run jest:coverage && npm run cy:run-unit && npm run coverage:merge && npm run coverage:merge-report",
    "coverage:merge": "shx mkdir coverage/__temp__ && shx cp ./coverage/jest/coverage-final.json coverage/__temp__/jest.json && shx cp ./coverage/cypress/coverage-final.json coverage/__temp__/cypress.json && nyc merge coverage/__temp__ coverage/merged/coverage-final.json && shx rm -rf coverage/__temp__",
    "coverage:merge-report": "nyc report --reporter=lcov --reporter=text --temp-dir=./coverage/merged --report-dir=./coverage/merged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn jest:staged"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
